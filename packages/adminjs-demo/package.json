{
  "name": "adminjs-demo",
  "version": "1.0.0",
  "main": "build/src/firebase.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "bundle": "NODE_ENV=production node bin/bundle",
    "start": "yarn build && node build/src/index.js",
    "dev": "concurrently \"wait-on build/src/index.js && nodemon node build/src/index.js\" \"yarn build --watch\"",
    "clean": "rm -rf build && mkdir build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "ff:serve": "yarn build && firebase emulators:start --only functions,hosting",
    "ff:shell": "yarn build && firebase functions:shell",
    "ff:start": "yarn shell",
    "ff:deploy": "yarn bundle && firebase deploy --only functions,hosting",
    "ff:logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/react": "^16.9.52",
    "@types/styled-components": "^5.1.4",
    "concurrently": "^5.2.0",
    "cypress": "^5.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "wait-on": "^5.2.0"
  },
  "dependencies": {
    "@adminjs/design-system": "2.0.1",
    "@adminjs/express": "^4.0.0",
    "@adminjs/firebase-functions": "4.0.0",
    "@adminjs/passwords": "2.0.0",
    "@adminjs/sequelize": "2.0.0",
    "@adminjs/upload": "2.0.1",
    "@sentry/node": "^5.26.0",
    "@sentry/tracing": "^5.26.0",
    "@types/quill": "^2.0.4",
    "adminjs": "5.0.1",
    "argon2": "^0.27.0",
    "connect-session-sequelize": "^7.0.3",
    "dotenv": "^8.2.0",
    "dotenv-json": "^1.0.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.0",
    "pg": "^8.3.0",
    "quill": "^1.3.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sequelize": "^6.3.5",
    "styled-components": "5.2.1"
  }
}
